@page
@model WebTest.Pages.Employee_ListingModel
@{

}

<h1>Employee</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Employee</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model.employeeList)
        {
            <tr>
                <td>@employee.empName</td>
                <td>@employee.email</td>
            </tr>
        }
    </tbody>
</table>
