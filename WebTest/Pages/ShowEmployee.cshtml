@page
@model WebTest.Pages.ShowEmployeeModel
@{
}

<div>
    <h1>Employees</h1>
    <p>Employee Information</p>
    <div class="table-responsive">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th class="bg-dark text-light">Employee ID</th>
                    <th class="bg-dark text-light">Name</th>
                    <th class="bg-dark text-light">Email</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var employee in Model.employeeList)
                {
                    <tr>
                        <td> @employee.empId</td>
                        <td> @employee.empName</td>
                        <td> @employee.email</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <br />
    <input type="button" id="btnBack" value="Back" class="btn btn-info" onclick="javascript:Back()" />
</div>

<script>
    function Back() {
        //var varBack = document.getElementById("btnBack").value;
        window.location.assign("EmployeeType");
    }
</script>